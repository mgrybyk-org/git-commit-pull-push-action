{
  "name": "git-commit-pull-push-action",
  "version": "1.2.6",
  "description": "Runs git add, git commit, git pull and git push",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "ncc build index.ts --target es2021 --license licenses.txt",
    "compile": "tsc",
    "check:compile": "tsc",
    "check:lint": "eslint .",
    "checks": "run-s check:*"
  },
  "keywords": [
    "github",
    "action",
    "git",
    "add",
    "commit",
    "pull",
    "push"
  ],
  "author": "mgrybyk",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
